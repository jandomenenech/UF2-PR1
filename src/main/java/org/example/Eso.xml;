    <title>Professor Layton and the Unwound Future</title>
    <releaseDate>Nov 27, 2008</releaseDate>
    <team>
      <member>['Level-5'</member>
      <member>'Nintendo of America']</member>
    </team>
    <rating>4.3</rating>
    <timesListed>378</timesListed>
